<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#bcbbbe" />

            <corners android:radius="3px" />

            <stroke android:width="1px" android:color="#a1a1a1" />
        </shape></item>
    <item><shape>
            <corners android:radius="3px" />

            <solid android:color="#efeef3" />

            <stroke android:width="1px" android:color="#bfbfc1" />
        </shape></item>

</selector>